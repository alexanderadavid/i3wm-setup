{
    "javascript.validate.enable": false, 
    "protoc": {
        "options": [
            "--proto_path=${workspaceRoot}/go/src", 
            "--proto_path=${env.GOPATH}/src"
        ]
    }, 
    "workbench.sideBar.location": "left", 
    "go.testFlags": [
        "-v"
    ], 
    "terminal.integrated.rendererType": "dom", 
    "window.menuBarVisibility": "hidden", 
    "typescript.disableAutomaticTypeAcquisition": true, 
    "workbench.activityBar.visible": false, 
    "go.useCodeSnippetsOnFunctionSuggestWithoutType": true, 
    "breadcrumbs.enabled": true, 
    "editor.fontFamily": "'Operator Mono Book', 'Fira Mono', Monaco, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'", 
    "security.workspace.trust.untrustedFiles": "open", 
    "editor.fontSize": 13, 
    "[typescriptreact]": {
        "editor.defaultFormatter": "numso.prettier-standard-vscode"
    }, 
    "window.zoomLevel": 2, 
    "telemetry.enableCrashReporter": false, 
    "mochaExplorer.logpanel": true, 
    "go.docsTool": "godoc", 
    "editor.formatOnSaveTimeout": 2000, 
    "prettier.singleQuote": true, 
    "[json]": {
        "editor.defaultFormatter": "numso.prettier-standard-vscode"
    }, 
    "terminal.integrated.cursorStyle": "line", 
    "[typescript]": {
        "editor.defaultFormatter": "numso.prettier-standard-vscode"
    }, 
    "files.autoSave": "afterDelay", 
    "go.liveErrors": {
        "delay": 500, 
        "enabled": true
    }, 
    "gitlens.advanced.telemetry.enabled": false, 
    "flow.useNPMPackagedFlow": true, 
    "javascript.format.semicolons": "remove", 
    "typescript.format.semicolons": "remove", 
    "go.formatTool": "goimports", 
    "gitlens.codeLens.enabled": false, 
    "typescript.updateImportsOnFileMove.enabled": "always", 
    "workbench.iconTheme": "vscode-icons", 
    "workbench.colorCustomizations": {
        "sideBar.background": "#000000", 
        "editorRuler.foreground": "#ff0000", 
        "tab.activeBackground": "#201f1f", 
        "editor.selectionBackground": "#6c40bebd", 
        "peekViewEditor.background": "#000000", 
        "sideBar.border": "#C7964D", 
        "editorGroupHeader.tabsBackground": "#000000", 
        "tab.activeForeground": "#ffffff", 
        "tab.activeBorder": "#6A7886", 
        "editor.findMatchHighlightBackground": "#7a89cc81", 
        "editorGroup.border": "#000000", 
        "statusBar.noFolderBackground": "#A9882D", 
        "editor.background": "#000000", 
        "editor.findMatchBackground": "#7a89ccc5", 
        "tab.inactiveBackground": "#000000", 
        "peekView.border": "#C7964D", 
        "statusBar.border": "#000000", 
        "tab.inactiveForeground": "#c1c5c6", 
        "statusBar.background": "#A9882D", 
        "statusBar.foreground": "#000000", 
        "statusBar.noFolderForeground": "#000000"
    }, 
    "go.toolsGopath": "~/Development/cwn/go", 
    "go.toolsManagement.autoUpdate": true, 
    "vsicons.dontShowNewVersionMessage": true, 
    "editor.acceptSuggestionOnCommitCharacter": false, 
    "workbench.statusBar.visible": true, 
    "terminal.integrated.fontFamily": "'Operator Mono Book', 'Fira Mono', Monaco, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'", 
    "prettier.jsxBracketSameLine": true, 
    "prettier.bracketSpacing": false, 
    "editor.fontLigatures": true, 
    "explorer.confirmDelete": false, 
    "javascript.updateImportsOnFileMove.enabled": "always", 
    "python.pythonPath": "/usr/bin/python2", 
    "runOnSave.statusMessageTimeout": 3000, 
    "python.formatting.autopep8Args": [
        "--max-line-length", 
        "100"
    ], 
    "editor.lineHeight": 22, 
    "editor.minimap.enabled": false, 
    "python.formatting.provider": "yapf", 
    "mochaExplorer.env": {
        "PUPPETEER_ARGS": "--no-sandbox --disable-dev-shm-usage"
    }, 
    "python.formatting.autopep8Path": "/usr/bin/autopep8", 
    "workbench.editor.enablePreview": false, 
    "go.formatFlags": [
        "-w"
    ], 
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    }, 
    "files.associations": {
        "*.jsx": "javascript", 
        "*.yaml": "plain text"
    }, 
    "editor.tabSize": 2, 
    "[javascript]": {
        "editor.defaultFormatter": "numso.prettier-standard-vscode"
    }, 
    "prettier.trailingComma": "es5", 
    "go.lintTool": "golangci-lint", 
    "go.autocompleteUnimportedPackages": true, 
    "telemetry.enableTelemetry": false, 
    "editor.formatOnSave": true, 
    "diffEditor.ignoreTrimWhitespace": false, 
    "bracketPairColorizer.colorMode": "Independent", 
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }, 
    "terminal.integrated.fontSize": 13, 
    "go.useLanguageServer": true,
    "workbench.editor.untitled.hint": "hidden"
}