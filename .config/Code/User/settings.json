{
    "go.useLanguageServer": true,
    "go.lintTool": "golangci-lint",
    "go.autocompleteUnimportedPackages": true,
    "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
    "go.docsTool": "godoc",
    "go.liveErrors": {
        "delay": 500,
        "enabled": true
    },
    "go.formatTool": "goimports",
    "go.toolsGopath": "~/Development/cwn/go",
    "editor.formatOnSaveTimeout": 2000,
    "go.testFlags": [
        "-v"
    ],
    "javascript.validate.enable": false,
    "protoc": {
        "options": [
            "--proto_path=${workspaceRoot}/go/src",
            "--proto_path=${env.GOPATH}/src"
        ]
    },
    "terminal.integrated.rendererType": "dom",
    "window.menuBarVisibility": "hidden",
    "workbench.activityBar.visible": false,
    "breadcrumbs.enabled": true,
    "editor.fontFamily": "'Operator Mono Book', 'Fira Mono', Monaco, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 13,
    "telemetry.enableCrashReporter": false,
    "prettier.singleQuote": true,
    "gitlens.codeLens.enabled": false,
    "terminal.integrated.cursorStyle": "line",
    "files.autoSave": "afterDelay",
    "gitlens.advanced.telemetry.enabled": false,
    "flow.useNPMPackagedFlow": true,
    "runOnSave.statusMessageTimeout": 3000,
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorCustomizations": {
        "sideBar.background": "#000000",
        "editorRuler.foreground": "#ff0000",
        "tab.activeBackground": "#201f1f",
        "editor.selectionBackground": "#6c40bebd",
        "peekViewEditor.background": "#000000",
        "statusBar.noFolderForeground": "#0f1618",
        "sideBar.border": "#6B8A9F",
        "editorGroupHeader.tabsBackground": "#000000",
        "tab.activeForeground": "#ffffff",
        "editor.findMatchHighlightBackground": "#7a89cc81",
        "editorGroup.border": "#0f1618",
        "statusBar.noFolderBackground": "#55788A",
        "statusBar.foreground": "#0f1618",
        "editor.findMatchBackground": "#7a89ccc5",
        "tab.inactiveBackground": "#000000",
        "peekView.border": "#6B8A9F",
        "statusBar.border": "#0f1618",
        "tab.inactiveForeground": "#dedde2",
        "statusBar.background": "#55788A",
        "editor.background": "#000000",
        "tab.activeBorder": "#9AA5B4"
    },
    "vsicons.dontShowNewVersionMessage": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "workbench.statusBar.visible": true,
    "terminal.integrated.fontFamily": "'Operator Mono Book', 'Fira Mono', Monaco, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "prettier.jsxBracketSameLine": true,
    "prettier.bracketSpacing": false,
    "editor.fontLigatures": true,
    "explorer.confirmDelete": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "python.pythonPath": "/usr/bin/python2",
    "typescript.disableAutomaticTypeAcquisition": true,
    "python.formatting.autopep8Args": [
        "--max-line-length",
        "100"
    ],
    "editor.lineHeight": 22,
    "editor.minimap.enabled": false,
    "python.formatting.provider": "yapf",
    "python.formatting.autopep8Path": "/usr/bin/autopep8",
    "workbench.editor.enablePreview": false,
    "terminal.integrated.fontSize": 13,
    "mochaExplorer.env": {
        "PUPPETEER_ARGS": "--no-sandbox --disable-dev-shm-usage"
    },
    "editor.tabSize": 2,
    "files.associations": {
        "*.jsx": "javascript",
        "*.yaml": "plain text"
    },
    "prettier.trailingComma": "es5",
    "telemetry.enableTelemetry": false,
    "editor.formatOnSave": true,
    "bracketPairColorizer.colorMode": "Independent",
    "workbench.sideBar.location": "left",
    "go.formatFlags": [
        "-w"
    ],
    "typescript.format.semicolons": "remove",
    "javascript.format.semicolons": "remove",
    "[typescript]": {
        "editor.defaultFormatter": "numso.prettier-standard-vscode"
    },
    "window.zoomLevel": 1
}