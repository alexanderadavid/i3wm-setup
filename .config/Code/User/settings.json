{
    "python.analysis.autoImportCompletions": true,
    "python.analysis.fixAll": [
        "source.unusedImports",
    ],
    "jest.autoRun": {
        "onStartup": false,
        "onSave": false,
        "watch": false
    },
    "javascript.validate.enable": false,
    "go.toolsManagement.autoUpdate": true,
    "go.testTimeout": "300s",
    "remote.SSH.showLoginTerminal": true,
    "breadcrumbs.enabled": true,
    "editor.fontFamily": "'Operator Mono Book', 'Fira Mono', Monaco, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 13,
    "editor.lineHeight": 22,
    "explorer.confirmDragAndDrop": false,
    "prettier.singleQuote": true,
    "terminal.integrated.cursorStyle": "line",
    "go.formatTool": "goimports",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "python.envFile": "${workspaceFolder}/.env",
    "workbench.colorCustomizations": {
        "sideBar.background": "#000000",
        "editorRuler.foreground": "#ff0000",
        "tab.activeBackground": "#201f1f",
        "editor.selectionBackground": "#6c40bebd",
        "peekViewEditor.background": "#000000",
        "statusBar.noFolderForeground": "#000000",
        "sideBar.border": "#1F8D71",
        "editorGroupHeader.tabsBackground": "#000000",
        "tab.activeForeground": "#ffffff",
        "editor.findMatchHighlightBackground": "#7a89cc81",
        "editorGroup.border": "#000000",
        "statusBar.noFolderBackground": "#6B8A2E",
        "statusBar.foreground": "#000000",
        "editor.findMatchBackground": "#7a89ccc5",
        "tab.inactiveBackground": "#000000",
        "peekView.border": "#1F8D71",
        "statusBar.border": "#000000",
        "tab.inactiveForeground": "#83ccd4",
        "statusBar.background": "#6B8A2E",
        "editor.background": "#000000",
        "tab.activeBorder": "#000000"
    },
    "go.testFlags": [
        "-v"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "workbench.statusBar.visible": true,
    "terminal.integrated.fontFamily": "'Operator Mono Book', 'Fira Mono', Monaco, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "prettier.bracketSpacing": false,
    "editor.fontLigatures": true,
    "explorer.confirmDelete": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.sideBar.location": "left",
    "typescript.disableAutomaticTypeAcquisition": true,
    "gitlens.codeLens.enabled": false,
    "editor.minimap.enabled": false,
    "mochaExplorer.env": {
        "PUPPETEER_ARGS": "--no-sandbox --disable-dev-shm-usage"
    },
    "workbench.editor.enablePreview": false,
    "go.formatFlags": [
        "-w"
    ],
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.json": "default"
    },
    "files.associations": {
        "*.jsx": "javascript",
        "*.yaml": "plain text"
    },
    "editor.tabSize": 2,
    "prettier.trailingComma": "es5",
    "go.lintTool": "golangci-lint",
    "editor.formatOnSave": false,
    // TODO this could be theme colors
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.fontSize": 13,
    "go.useLanguageServer": true,
    "bracketPairColorizer.depreciation-notice": false,
    // "python.defaultInterpreterPath": "/usr/bin/python2",
    "js/ts.implicitProjectConfig.checkJs": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "jestrunner.jestCommand": "npm test -- ",
    "jestrunner.debugOptions": {},
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.inlineSuggest.enabled": true,
    "githubPullRequests.pullBranch": "never",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    // "[typescriptreact]": {
    //     "editor.defaultFormatter": "vscode.typescript-language-features"
    // },
    // "[javascript]": {
    //     "editor.defaultFormatter": "vscode.typescript-language-features"
    // },
    "git.openRepositoryInParentFolders": "never",
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": false,
        "scminput": false
    },
    // "[typescript]": {
    //     "editor.defaultFormatter": "esbenp.prettier-vscode"
    // },
    "workbench.activityBar.location": "hidden",
    "workbench.editor.empty.hint": "hidden",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.menuBarVisibility": "toggle"
}